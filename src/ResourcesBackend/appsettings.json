{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:5243"
      }
    },
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "BackendConfig": {
    "ResourcesConfig": {
      "SubscriptionId": "SUBID",
      "ResourceGroupName": "RGNAME"
    },
    "SecurityConfig": {
      "TenantId": "AAD_TENANT",
      "ClientId": "AAD_CLIENTID",
      "ClientSecret": "AAD_CLIENT_SECRET"
    }
  }
}